{
    "Logging": {
        "LogLevel": {
            "Default": "Debug",
            "Microsoft": "Debug",
            "Microsoft.AspNetCore": "Debug"
        }
    },
    "TestOptions": {
        "TestSetting": "Test",
        "Url": "https://support.example.com",
        "Email": "help@support.example.com",
        "PhoneNumber": "+7(812)-3452489"
    },
    "ServiceOptions": {
        "IsSecured": true
    },
    "Redis": {
        "URL": "localhost:6379"
    },
    "AllowedHosts": "*",
    "Serilog": {
        "Using": [ "Serilog.Sinks.Console", "Serilog.Sinks.File" ],
        "MinimumLevel": "Debug",
        "WriteTo": [
            { "Name": "Console" },
            {
                "Name": "File",
                "Args": { "path": "Logs/log.txt" }
            }
        ]
    }
}
